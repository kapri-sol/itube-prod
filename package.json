{
  "name": "itube-prod",
  "version": "1.0.0",
  "description": "video sharing web site",
  "main": "index.ts",
  "repository": "https://github.com/kapri-sol/itube-prod",
  "author": "kapri-sol <ksm6902@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/dotenv": "^6.1.1",
    "@types/graphql-upload": "^8.0.3",
    "@types/jsonwebtoken": "^8.3.3",
    "@types/mkdirp": "^0.5.2",
    "@types/node": "^12.7.3",
    "@types/passport": "^1.0.1",
    "@types/passport-jwt": "^3.0.1",
    "babel-runtime": "^6.26.0",
    "dotenv": "^8.1.0",
    "gql-merge": "^0.0.6",
    "graphql-to-typescript": "^0.1.2",
    "nodemon": "^1.19.1",
    "ts-node": "^8.4.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.6.2"
  },
  "scripts": {
    "heroku-postbuild": "yarn types & yarn build",
    "deploy": "prisma deploy",
    "generate": "prisma generate",
    "prisma": "yarn run deploy && yarn run generate",
    "prebuild": "rm -rf dist",
    "build": "tsc",
    "postbuild": "copy src/api/**/*.graphql dist/src/api",
    "pretypes": "gql-merge --out-file ./src/schema.graphql ./src/api/**/*.graphql",
    "types": "graphql-to-typescript ./src/schema.graphql ./src/types/graph.d.ts",
    "predev": "yarn run types",
    "dev": "cd src && nodemon --exec ts-node index.ts -e ts, graphql",
    "start": "node dist/src/index.js",
    "server": "cd src &&  ts-node index.ts",
    "client": "yarn run start --prefix client"
  },
  "dependencies": {
    "bcrypt": "^3.0.6",
    "copy": "^0.3.2",
    "cors": "^2.8.5",
    "fluent-ffmpeg": "^2.1.2",
    "fs": "^0.0.1-security",
    "graphql-tools": "^4.0.5",
    "graphql-yoga": "^1.18.3",
    "helmet": "^3.20.1",
    "jsonwebtoken": "^8.5.1",
    "merge-graphql-schemas": "^1.7.0",
    "mkdirp": "^0.5.1",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "prisma-client-lib": "^1.34.8",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.2"
  },
  "engines": {
    "node": "10.16.3",
    "yarn": "1.17.3",
    "npm": "6.12.0",
    "prisma": "1.34.8",
    "docker-compose": "1.24.1"
  }
}
